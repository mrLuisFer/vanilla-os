{
  "name": "archcraft-webos",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "sass": "1.44.0",
    "svelte": "^3.44.0",
    "svelte-check": "2.2.10",
    "svelte-language-server": "0.14.15",
    "svelte-preprocess": "4.9.8",
    "typescript": "4.5.2",
    "vite": "^2.7.0"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "svelte-check",
      "npm run lint:fix"
    ],
    "*.js": "eslint --cache --fix",
    "*.svelte": "prettier --write"
  }
}
